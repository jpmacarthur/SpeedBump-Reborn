<UserControl x:Class="SpeedBump.Warning"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SpeedBump"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <TabControl Name="reports_TC" Visibility="Collapsed"></TabControl>
        <Button Height="30" Width="30" Name="reports_BT" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Click="Button_Click" VerticalAlignment="Bottom" HorizontalAlignment="Right">
        <Image x:Name="WarningStatus" Source="{Binding Status}"></Image>
        </Button>
    </Grid>
</UserControl>
